					"expression": {
						"value": "@equals(variables('IsLastRequest'),true)",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Check AnalyticsRequestDates",
							"type": "AzureFunctionActivity",
							"dependsOn": [],
