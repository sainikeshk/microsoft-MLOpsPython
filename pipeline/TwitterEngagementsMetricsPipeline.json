								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"functionName": "TwitterAnalyticsRequestDatesCheck",
								"method": "GET",
								"headers": {
									"StartDate": {
										"value": "@variables('StartDate')",
										"type": "Expression"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "TwitterAnalyticsRequestDatesCheck",
								"type": "LinkedServiceReference"
							}
						},
						{
							"name": "Set IsLastReq",
