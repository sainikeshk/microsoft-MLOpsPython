{
	"name": "nseLive",
	"properties": {
		"activities": [
			{
				"name": "copyBinaryfiles",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "SftpReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@concat('CM30/DATA/',formatDateTime(utcnow(),'MMMMddyyyy'))",
								"type": "Expression"
							},
							"wildcardFileName": "*.gz",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "sftpBinary",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "binaryBlobStorage",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Custom1",
				"type": "Custom",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"command": "cmd /c \"pip install azure-storage-blob pandas\"",
					"resourceLinkedService": {
						"referenceName": "sftpbinaryBlob",
						"type": "LinkedServiceReference"
					},
					"referenceObjects": {
						"linkedServices": [],
						"datasets": []
					}
				},
				"linkedServiceName": {
					"referenceName": "batchnse",
					"type": "LinkedServiceReference"
				}
			}
		],
		"annotations": []
	}
}