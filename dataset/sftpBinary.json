{
	"name": "sftpBinary",
	"properties": {
		"linkedServiceName": {
			"referenceName": "SftpConnLs",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "SftpLocation",
				"fileName": {
					"value": "@concat(activity('mktTrig').output.fileNo,'.mkt.gz')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('CM30/DATA/',formatDateTime(utcnow(),'MMMMddyyyy'))",
					"type": "Expression"
				}
			}
		}
	}
}